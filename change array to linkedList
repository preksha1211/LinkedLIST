/*
// Representation of a node
class Node {
    int data;
    Node next;
    Node (int d) {
       data = d;
       next = null;
    }
};
*/
class Solution {
    public Node arrayToList(int arr[]) {
        Node head=new Node(arr[0]);
        int n=arr.length;
        Node temp=head;
        for(int i=1;i<n;i++){
            Node nn=new Node(arr[i]);
            temp.next=nn;
            temp=nn;
        }
        return head;
        
    }
}
